{
  "name": "pubcrawl-src",
  "version": "0.0.1-alpha",
  "description": "A library that simplifies the pub-sub model for TypeScript and JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "npx rimraf ./dist && npx tsc",
    "change-name": "cd dist && npm pkg set name='pubcrawl' && cd ..",
    "copy": "cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md",
    "postbuild": "npm run copy && npm run change-name",
    "prebuild": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ej-shafran/pubcrawl.git"
  },
  "keywords": [
    "pub-sub",
    "publisher-subscriber",
    "subscribe",
    "sub",
    "pub",
    "publisher"
  ],
  "author": "ej-shafran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ej-shafran/pubcrawl/issues"
  },
  "homepage": "https://github.com/ej-shafran/pubcrawl#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "types": "index.d.ts"
}
